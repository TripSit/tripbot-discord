{
  "name": "@tripsit/tripbot-discord",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist",
    "dev": "NODE_ENV=development nodemon -w src/**/* -e ts,json --exec 'node --inspect -r ts-node/register src/index.ts'",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "NODE_ENV=test jest",
    "lint": "eslint .",
    "migrate:make": "knex --knexfile knexfile.ts -x ts migrate:make",
    "knex:migrate:latest": "knex --knexfile knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback"
  },
  "author": "",
  "license": "APL-2",
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^8.2.0",
    "knex": "^0.95.4",
    "knex-stringcase": "^1.4.5",
    "sqlite3": "^5.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
