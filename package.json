{
  "name": "@tripsit/tripbot-discord",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist",
    "build": "rm -rf dist && tsc",
    "dev": "nodemon ./src/index.ts",
    "test": "npm run typecheck && npm run lint && npm run test:unit",
    "test:unit": "NODE_ENV=test jest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "db:migrate:latest": "knex --knexfile ./src/knexfile.ts migrate:latest",
    "db:migrate:make": "knex --knexfile ./src/knexfile.ts migrate:make -x ts --"
  },
  "author": "SevenCats <sevencats@tripsit.me>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "make-error": "^1.3.6",
    "pg": "^8.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.4.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.4",
    "jest-discordjs-mocks": "^1.0.2",
    "nodemon": "^2.0.7",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
